    "build": "rollup -c"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^18.19.33",
    "rollup": "^4.9.6",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  }
}
